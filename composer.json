{
  "name": "3xw/cakephp-elastic-search",
  "description": "CakePHP pages plugins",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=8.0",
    "cakephp/cakephp": "^5.0",
    "cakephp/elastic-search": "3.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Trois\\ElasticSearch\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trois\\ElasticSearch\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  },
  "minimum-stability": "dev"
}
